# SPDX-License-Identifier: Apache-2.0
[run]
source = app
omit =
    */tests/*
    */__pycache__/*
    conftest.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise NotImplementedError
    if TYPE_CHECKING:
    if __name__ == .__main__.:

# TODO: increase to 70% as more integration tests are added
fail_under = 60
